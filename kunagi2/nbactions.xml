<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>rebuild</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
                
                
                
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-ScrumModeller</actionName>
            <displayName>ScrumModeller</displayName>
            <recursive>false</recursive>
            <goals>
                <goal>generate-sources</goal>
            </goals>
            <activatedProfiles>
                <activatedProfile>ScrumModeller</activatedProfile>
            </activatedProfiles>
        </action>
        <action>
            <actionName>test</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>test</goal>
                <goal>-X</goal>
            </goals>
        </action>
        <action>
            <actionName>test.single</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>test-compile</goal>
                <goal>surefire:test</goal>
                <goal>-e</goal>
            </goals>
            <properties>
                <test>${packageClassName}</test>
            </properties>
        </action>
    </actions>
